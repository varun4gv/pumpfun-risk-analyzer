{
  "name": "pumpfun-risk-analyzer",
  "version": "1.0.0",
  "description": "A comprehensive risk analysis and detection system for pump.fun tokens",
  "main": "index.js",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "cd backend && python main.py",
    "client": "cd frontend && npm start",
    "build": "cd frontend && npm run build",
    "test": "npm run test:backend && npm run test:frontend",
    "test:backend": "cd backend && python -m pytest",
    "test:frontend": "cd frontend && npm test",
    "lint": "npm run lint:backend && npm run lint:frontend",
    "lint:backend": "cd backend && flake8 . && black --check .",
    "lint:frontend": "cd frontend && npm run lint",
    "format": "npm run format:backend && npm run format:frontend",
    "format:backend": "cd backend && black .",
    "format:frontend": "cd frontend && npm run format",
    "setup": "npm install && cd frontend && npm install && cd ../backend && pip install -r requirements.txt",
    "docker:build": "docker-compose build",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down"
  },
  "keywords": [
    "pumpfun",
    "solana",
    "defi",
    "risk-analysis",
    "rug-pull-detection",
    "token-analysis",
    "blockchain",
    "cryptocurrency",
    "security",
    "analytics"
  ],
  "author": "Your Name <your.email@example.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/pumpfun-risk-analyzer.git"
  },
  "bugs": {
    "url": "https://github.com/yourusername/pumpfun-risk-analyzer/issues"
  },
  "homepage": "https://github.com/yourusername/pumpfun-risk-analyzer#readme",
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "engines": {
    "node": ">=18.0.0",
    "python": ">=3.8.0"
  }
}
